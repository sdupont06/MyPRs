{
  "expo": {
    "name": "MyPRs",
    "slug": "myprs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MyPRs_Icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "PowerliftingAppBuild"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MyPRs_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.MyPRs.lift"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/MyPRs_Splash.png",
          "dark": {
            "image": "./assets/images/MyPRs_Splash.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ebd6cfad-66c2-492f-97d5-3b5a399fbe45"
      }
    },
    "owner": "sdupont"
  }
}
